			</wsdl:input>
			<wsdl:output>
				<soap:body use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
			</wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="KYCRequest">
			<soap:operation soapAction="urn:#NewOperation"/>
			<wsdl:input>
				<soap:body use="literal"/>
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal"/>
			</wsdl:output>
		</wsdl:operation>
	</binding>
	<service name="RegisterService">
		<documentation>Service d'inscription OM-Banking </documentation>
		<port name="RegisterPort" binding="ns1:RegisterBinding">
			<soap:address location="https://b2w-sb.orange-money.com/register/?bic=BNKSXGWE"/>
		</port>
